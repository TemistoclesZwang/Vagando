{
    "openapi": "3.0.0",
    "info": {
        "title": "Vagando api",
        "description": "this a documentation of Vagando api",
        "version": "1.0.0",
        "contact": {
            "email": "temis2st@gmail.com"
        }
    },
    "paths": {
        "/login": {
            "post": {
                "tags": [
                    "Autenticação"
                ],
                "summary": "Realizar login do usuário",
                "description": "Endpoint para autenticar o usuário com email e senha.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "body",
                        "in": "body",
                        "description": "Dados de login do usuário",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "properties": {
                                "email": {
                                    "type": "string"
                                },
                                "senha": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Sucesso. O usuário foi autenticado."
                    },
                    "401": {
                        "description": "Falha na autenticação. Email ou senha inválidos."
                    },
                    "500": {
                        "description": "Erro interno do servidor."
                    }
                }
            }
        },
        "/cadastro/aluno": {
            "post": {
                "tags": [
                    "Formulário"
                ],
                "summary": "Preencher formulário",
                "description": "Endpoint para preencher um formulário.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "body",
                        "in": "body",
                        "description": "Dados do formulário preenchido pelo usuário",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "properties": {
                                "email": {
                                    "type": "string"
                                },
                                "senha": {
                                    "type": "string"
                                },
                                "confirmar senha": {
                                    "type": "string"
                                },
                                "matricula": {
                                    "type": "string"
                                },
                                "nome": {
                                    "type": "string"
                                },
                                "nascimento": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Sucesso. O formulário foi preenchido com sucesso."
                    },
                    "400": {
                        "description": "Requisição inválida. Os dados do formulário estão faltando ou são inválidos."
                    },
                    "500": {
                        "description": "Erro interno do servidor."
                    }
                }
            }
        },
        "/cadastro/empresa": {
            "post": {
                "tags": [
                    "Formulário"
                ],
                "summary": "Preencher formulário",
                "description": "Endpoint para preencher um formulário de cadastro para empresa.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "body",
                        "in": "body",
                        "description": "Dados do formulário preenchido pelo usuário",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "properties": {
                                "email": {
                                    "type": "string"
                                },
                                "senha": {
                                    "type": "string"
                                },
                                "confirmar senha": {
                                    "type": "string"
                                },
                                "cnpj": {
                                    "type": "string"
                                },
                                "nome": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Sucesso. O formulário foi preenchido com sucesso."
                    },
                    "400": {
                        "description": "Requisição inválida. Os dados do formulário estão faltando ou são inválidos."
                    },
                    "500": {
                        "description": "Erro interno do servidor."
                    }
                }
            }
        },
        "/feed/pagina": {
            "get": {
                "tags": [
                    "Feed"
                ],
                "summary": "Recuperar dados do card do feed",
                "description": "Endpoint para recuperar os dados do card no feed",
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "parameters": [
                    {
                        "name": "numeroDaPagina",
                        "in": "query",
                        "description": "Número da página",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Sucesso. Os dados do card do feed foram recuperados com sucesso."
                    },
                    "401": {
                        "description": "Não autorizado. O usuário não está logado."
                    },
                    "500": {
                        "description": "Erro interno do servidor."
                    }
                }
            }
        },
        "/vagas": {
            "post": {
                "tags": [
                    "Vagas"
                ],
                "summary": "Criar nova vaga",
                "description": "Endpoint para criar uma nova vaga.",
                "parameters": [
                    {
                        "name": "body",
                        "in": "body",
                        "description": "Dados da vaga a ser criada",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "properties": {
                                "idDaEmpresa": {
                                    "type": "string"
                                },
                                "nomeEmpresa": {
                                    "type": "string"
                                },
                                "tituloVaga": {
                                    "type": "string"
                                },
                                "salario": {
                                    "type": "number"
                                },
                                "date_time": {
                                    "type": "string",
                                    "format": "date-time"
                                }
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Sucesso. A vaga foi criada com sucesso."
                    },
                    "400": {
                        "description": "Requisição inválida. Os dados da vaga estão faltando ou são inválidos."
                    },
                    "500": {
                        "description": "Erro interno do servidor."
                    }
                }
            }
        },
        "/vagas/historico": {
            "get": {
                "tags": [
                    "Vagas"
                ],
                "summary": "Obter histórico de vagas",
                "description": "Endpoint para obter o histórico de vagas criadas pela empresa.",
                "security": [
                    {
                        "bearerAuth": []
                    }
                ],
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "Sucesso. Retorna o histórico de vagas.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "id": {
                                                "type": "integer",
                                                "description": "ID da vaga"
                                            },
                                            "idDaEmpresa": {
                                                "type": "string",
                                                "description": "ID da empresa responsável pela vaga"
                                            },
                                            "nomeEmpresa": {
                                                "type": "string",
                                                "description": "Nome da empresa responsável pela vaga"
                                            },
                                            "tituloVaga": {
                                                "type": "string",
                                                "description": "Título da vaga"
                                            },
                                            "salario": {
                                                "type": "number",
                                                "description": "Salário da vaga"
                                            },
                                            "date_time": {
                                                "type": "string",
                                                "format": "date-time",
                                                "description": "Data e hora da criação da vaga"
                                            }
                                        },
                                        "required": [
                                            "id",
                                            "idDaEmpresa",
                                            "nomeEmpresa",
                                            "tituloVaga",
                                            "salario",
                                            "date_time"
                                        ]
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Não autorizado. O token de autenticação é inválido ou expirou."
                    },
                    "500": {
                        "description": "Erro interno do servidor."
                    }
                }
            }
        }
    }
}